<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cursojava.fit.ads.crud">
	<class name="Funcionario" table="Funcionario">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="nome" />
		<property name="identidade" />
		<property name="nascimento" type="date" column="dtNasc" />
		<property name="dataAdmissao" type="date" column="dtAdmissao" />
		<property name="salario" />
	</class>

</hibernate-mapping>