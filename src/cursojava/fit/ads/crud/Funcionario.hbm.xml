<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cursojava.fit.ads.crud">
	<class name="Funcionario" table="funcionario">
		<id name="codigo" column="codigo">
	    	<generator class="increment"/>
    	</id>
    	<property name="nome" column="nome" />
    	<property name="identidade" column="identidade" />
    	<property name="nascimento" column="dataNasc" />
		<property name="admissao" column="dataAdmissao" />
		<property name="salario" column="salario" />
	</class>

</hibernate-mapping>